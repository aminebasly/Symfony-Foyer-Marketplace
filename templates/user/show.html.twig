            <tr>
                <th>IdEtudiant</th>
                <td>{{ user.idEtudiant }}</td>
            </tr>
            <tr>
                <th>Specialite</th>
                <td>{{ user.specialite }}</td>
            </tr>
            <tr>
                <th>IdTechnicien</th>
                <td>{{ user.idTechnicien }}</td>
            </tr>
            <tr>
                <th>Service</th>
                <td>{{ user.service }}</td>
            </tr>
